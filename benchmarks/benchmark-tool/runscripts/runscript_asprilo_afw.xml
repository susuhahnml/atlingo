<runscript output="scripts_asprilo_awf__h-{H}">

	<machine name="komputer" cpu="24x8xE5520@2.27GHz" memory="24GB"/>
	
  	<config name="seq-asprilo" template="templates/seq-asprilo.sh"/>
  
	<system name="compute_afw" version="1" measures="clasp" config="seq-asprilo">

		<setting name="square release__h-{H}" cmdline="{H} square_release" tag="basic" />
		<setting name="horizontal before vertical__h-{H}" cmdline="{H} horizontally_before_vertically" tag="basic" />
		<setting name="vertical before vhorizontal__h-{H}" cmdline="{H} vertically_before_horizontally" tag="basic" />
		<setting name="never both direction__h-{H}" cmdline="{H} never_both_directions" tag="basic" />
		<setting name="eventually 5xright__h-{H}" cmdline="{H} eventually_r_r_r_r_r" tag="basic" />

    </system>

	<seqjob name="seq-gen" timeout="900" runs="1" script_mode="timeout" walltime="50:00:00" parallel="1"/>

	<benchmark name="asprilo-benchmark">
		<folder path="../../env/asprilo/asprilo-abstraction-encodings/instances/md/structured/"> </folder> 
	</benchmark>
	
	<project name="clingo-seq-job" job="seq-gen">
		<runtag machine="komputer" benchmark="asprilo-benchmark" tag="basic"/>
	</project>

</runscript>

