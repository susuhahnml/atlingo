<runscript output="scripts_asprilo_afw__h-45__n-1">

	<machine name="komputer" cpu="24x8xE5520@2.27GHz" memory="24GB"/>
	
  	<config name="seq-asprilo" template="templates/seq-asprilo.sh"/>
  
	<system name="compute_afw" version="1" measures="clasp" config="seq-asprilo">

		<setting name="square_release" cmdline="45 1 square_release" tag="basic" />
		<setting name="horizontal_before_vertical" cmdline="45 1 horizontally_before_vertically" tag="basic" />
		<setting name="vertical_before_horizontal" cmdline="45 1 vertically_before_horizontally" tag="basic" />
		<setting name="never_both_direction" cmdline="45 1 never_both_directions" tag="basic" />
		<setting name="eventually_5xright" cmdline="45 1 eventually_r_r_r_r_r" tag="basic" />

    </system>

	<seqjob name="seq-gen" timeout="900" runs="1" script_mode="timeout" walltime="50:00:00" parallel="1"/>

	<benchmark name="asprilo-benchmark">
		<folder path="../asprilo_instances/"> </folder> 
	</benchmark>
	
	<project name="clingo-seq-job" job="seq-gen">
		<runtag machine="komputer" benchmark="asprilo-benchmark" tag="basic"/>
	</project>

</runscript>

