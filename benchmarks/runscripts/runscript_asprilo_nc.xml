<runscript output="output/nc__h-{H}__n-{N}">

	<machine name="komputer" cpu="24x8xE5520@2.27GHz" memory="24GB"/>
	
	<config name="seq-asprilo" template="../programs/seq-asprilo.sh"/>
  
	<system name="compute_nc" version="1" measures="clingo" config="seq-asprilo">

		<setting name="without_constraint" cmdline="{H} {N}" tag="basic" />

    </system>

	<pbsjob name="pbs-gen" partition="short" timeout="1200" runs="1" script_mode="timeout" walltime="00:50:00" cpt="4"/>

	<benchmark name="asprilo-benchmark">
		<folder path="/home/hahnmartinlu/temporal-automata/atlingo/benchmarks/asprilo_instances/"> </folder> 
	</benchmark>
	
	<project name="temporal-automata" job="pbs-gen">
		<runtag machine="komputer" benchmark="asprilo-benchmark" tag="basic"/>
	</project>

</runscript>

